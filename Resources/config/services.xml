<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="gnemes.search.manager" class="Gnemes\SearchBundle\Search\SearchManager">
            <argument>"@service_container"</argument>
        </service>
        
        <service id="gnemes.search.provider.factory" class="Gnemes\SearchBundle\Provider\ProviderFactory">
            <argument>"@service_container"</argument>
        </service>
    </services>
</container>
